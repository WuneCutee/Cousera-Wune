{
    "manifest_version": 3,
    "name": "Coursera Wune Pro",
    "version": "5.7",
    "permissions": ["storage", "sidePanel", "declarativeNetRequest", "cookies", "tabs", "activeTab"],
    "host_permissions": ["*://*.coursera.org/*", "https://gist.githubusercontent.com/*"],
    "background": { "service_worker": "background.js" },
    "side_panel": { "default_path": "sidepanel.html" },
    "action": { "default_title": "Open Coursera Wune", "default_icon": "icons/logo.png" },
    "icons": { "128": "icons/logo.png" }
}